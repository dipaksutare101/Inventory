@model  Inventory.Models.Registration
@{
    ViewBag.Title = "Register";
}

<h2>Register</h2>


@using(Html.BeginForm("Create","Register",FormMethod.Post))
{
<table>
    <tr>
        <td>Name</td>
        <td>@Html.TextBoxFor(m => m.name)</td>
    </tr>
    <tr>
        <td>Gender</td>
        <td>Male @Html.RadioButtonFor(m => m.Gender, "Male")</td>
        <td>FeMale @Html.RadioButtonFor(m => m.Gender, "FeMale")</td>
    </tr>
    <tr>
        <td>City</td>
        <td>@Html.DropDownListFor(m => m.cityid, (IEnumerable<SelectListItem>)ViewBag.city)</td>
    </tr>
    <tr>
        <td>IsStudent</td>
        <td>@Html.CheckBoxFor(m => m.isstudent)</td>
    </tr>
    <tr>
        <td><input type="submit" name="name" value="Save" /></td>
        
    </tr>
</table>
    

}
